{  
  "extends": "airbnb-base",
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "always"],
    "semi-spacing": [2, {"before": false,"after": true}],
    "no-empty": [0],
    "no-undef": [0],
    "no-shadow": [0],
    "indent": [0],
    "global-require": [0],
    "import/no-dynamic-require": [0],
    "import/prefer-default-export": [0],
    "no-else-return": [0],
    "import/no-extraneous-dependencies": [0],
    "no-use-before-define": [0],
    "no-multi-assign": [0],
    "import/extensions": [0],
    "import/no-unresolved": [0],
    "class-methods-use-this": [0],
    "no-param-reassign": [0],
    "func-names": [0],
    "consistent-return": [0],
    "arrow-body-style": [0],
    "no-restricted-syntax": [0],
    "no-useless-escape": [0],
    "one-var-declaration-per-line": [0],
    "one-var": [0]
  }
}
